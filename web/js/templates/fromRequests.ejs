<% var i = 0;
for(i = 0;i < requests.length;i++) {
    var fullName = requests[i].from.firstName + ' ' + requests[i].from.lastName;
%>
    <div>
        <%if(requests[i].from.diverType == 'INSTRUCTOR') {%>
        <a id="<%=requests[i].id %>_viewFromRequest" class="panel-text"><%=fullName %><b>(Instructor)</b></a>
        <% } else {%>
        <a id="<%=requests[i].id %>_viewFromRequest" class="panel-text"><%=fullName %></a>
        <% } %>
        <button class="userInfo-button reg-button" id="<%=requests[i].id %>_acceptRequest">
            <%=labels["cmas.face.client.social.friendRequest.accept"]%>
        </button>
        <button class="userInfo-button reg-button" id="<%= requests[i].id %>_rejectRequest">
            <%=labels["cmas.face.client.social.friendRequest.reject"]%>
        </button>
    </div>
<% } %>