<% var records = recordsInfo.records;
var i;
for(i = 0; i < records.length;i++) {
    var diver = records[i].diver;
    var fullName = diver.firstName + ' ' + diver.lastName;
    var userpicSrc;
    if (diver.photo == null) {
        userpicSrc = '/i/no_img.png';
    } else {
        userpicSrc = 'data:image/png;base64,' + diver.photo;
    }

    var starCnt = 0;
    if (records[i].score == 'ONE_STAR') {
        starCnt = 1;
    } else if (records[i].score == 'TWO_STAR') {
        starCnt = 2;
    } else if (records[i].score == 'THREE_STAR') {
        starCnt = 3;
    } else if (records[i].score == 'FOUR_STAR') {
        starCnt = 4;
    } else if (records[i].score == 'FIVE_STAR') {
        starCnt = 5;
    }
    var diveSpot = records[i].diveSpot.name + ", " + records[i].diveSpot.country.name;
    var note = records[i].note;
    var visibleNote;
    var invisibleNote;
    if (note.length > 90) {
        visibleNote = note.substring(0, 90);
        invisibleNote = note.substring(90, note.length);
    }
    else {
        visibleNote = note;
        invisibleNote = null;
    }
%>
<div class="logbookRecord" id="<%=recordsInfo.containerId %>logbookRecord_<%=records[i].id %>">
    <div class="clearfix">
        <div class="logbookFeed-elem-left">
            <img class="userpic userpicFeed" src="<%=userpicSrc %>"/>

            <div class="dialog-triangle"></div>
        </div>
        <div class="logbookFeed-elem-right">
            <p><span class="logbookFeed-elem-right-line panelFeed-text"><%=fullName %></span></p>
            <% if(recordsInfo.isMyRecords){
            %>
            <% var j;
            for(j = 0;j < starCnt;j++) {
            %>
            <img class="diveScoreFeed" src="/i/heart-small-full.png"/>
            <% } %>
            <%
            for(j = starCnt;j < 5;j++) {
            %>
            <img class="diveScoreFeed" src="/i/heart-small-empty.png"/>
            <% } %>
            <% } %>
            <p><span class="text"><%=diveSpot %></span></p>

            <p><span class="panelFeed-label"><%=records[i].diveDate %></span></p>
        </div>
        <% if(recordsInfo.isMyRecords && !records[i].digest){
        %>
        <div class="menuFeed" >
            <img src="/i/arrow_white_ico.png" id="<%=recordsInfo.containerId %>feedItemSettings_<%=records[i].id %>"/>
            <div class="menuFeedContent" id="<%=recordsInfo.containerId %>menuItemSettings_<%=records[i].id %>">
                <ul>
                    <li id="<%= recordsInfo.containerId %>edit_<%= records[i].id %>"><a href="#">Edit</a></li>
                    <li id="<%= recordsInfo.containerId %>delete_<%= records[i].id %>"><a href="#">Delete</a></li>
                </ul>
            </div>
        </div>
        <% } %>
    </div>

    <div class="panelFeed">
        <p>
            <span class="panelFeed-text"><%=visibleNote %></span>
            <a href="#" id="<%=recordsInfo.containerId %>more_<%=records[i].id %>"><span class="panelFeed-href">More&gt;</span></a>
            <span id="<%=recordsInfo.containerId %>invisibleNote_<%=records[i].id %>" style="display: none">
                <% if(invisibleNote){
                %>
                <span class="panelFeed-text"><%=invisibleNote %></span><br/>
                <% } %>
                <span class="panelFeed-label">Depth: </span>
                <span class="text"><%=records[i].depthMeters %> meters</span><br/>
                <span class="panelFeed-label">Duration: </span>
                <span class="text"><%=records[i].durationMinutes %> minutes</span><br/>
                <a href="#" id="<%=recordsInfo.containerId %>less_<%=records[i].id %>"><span
                            class="panelFeed-href"><&lt;Less</span></a>
            </span>
        </p>
        <% if(records[i].photoBase64){
        %>
        <img class="photoFeed" src="data:image/png;base64,<%=records[i].photoBase64 %>"/>
        <% } %>
    </div>
</div>
<% } %>
