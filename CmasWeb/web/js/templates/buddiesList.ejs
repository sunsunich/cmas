<% var i = 0;
for(i = 0;i < divers.length;i++) {
    var fullName = divers[i].firstName + ' ' + divers[i].lastName;
%>
<div class="buddieElem">
    <span class="buddieElemLeft" id="<%= divers[i].id %>_showDiver">
       <%= fullName %>
    </span>
    <div class="buddieElemRight" id="<%= divers[i].id %>_removeBuddie" src="/i/ic_clean_grey.png?v=<%= webVersion %>">

    </div>
</div>
<% } %>
